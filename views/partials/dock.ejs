<footer>
  <div class="dock">
    <% if (icons && icons.length) { %>
      <% icons.forEach(icon => { %>
        <div class="dock-icon">
          <img src="<%= icon.src %>" alt="<%= icon.alt %>">
        </div>
      <% }) %>
    <% } else { %>
      <!-- Default static icons if none provided -->
      <div class="dock-icon"><img src="/images/logo.png" alt="Logo 1"></div>
      <div class="dock-icon"><img src="/images/logo.png" alt="Logo 2"></div>
      <div class="dock-icon"><img src="/images/logo.png" alt="Logo 3"></div>
      <div class="dock-icon"><img src="/images/logo.png" alt="Logo 4"></div>
    <% } %>
  </div>
</footer>